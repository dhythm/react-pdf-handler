{
  "name": "react-pdf-handler",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "server:mergePdf:dev": "ts-node --project tsconfig.server.json server/mergePdf.ts",
    "server:mergePdf:prod": "tsc --project tsconfig.server.json  && node dist/server/mergePdf.js",
    "server:createPdf:dev": "ts-node --project tsconfig.server.json server/createPdf.ts",
    "server:createPdf:prod": "tsc --project tsconfig.server.json  && node dist/server/createPdf.js",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@airbnb/node-memwatch": "^2.0.0",
    "@react-pdf/renderer": "^3.1.6",
    "base64-stream": "^1.0.0",
    "commander": "^10.0.0",
    "lodash": "^4.17.21",
    "pdf-merger-js": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-pdf": "^6.2.2",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/airbnb__node-memwatch": "^2.0.0",
    "@types/base64-stream": "^1.0.2",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "2.8.4",
    "typescript": "^4.9.5",
    "vite": "^4.1.0"
  }
}
